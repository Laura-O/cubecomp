<% page_title 'Theme' %>

<%= render 'notice' %>

<ul>
  <% @theme_files.each do |theme_file| %>
    <li>
      <%= link_to theme_file.filename, edit_admin_competition_theme_file_path(current_competition, theme_file) %>
      (<%= link_to 'Destroy', admin_competition_theme_file_path(current_competition, theme_file), method: :delete, data: { confirm: 'Are you sure?' } %>)
    </Li>
  <% end %>
</ul>

<%= link_to 'New theme file', new_admin_competition_theme_file_path %>
