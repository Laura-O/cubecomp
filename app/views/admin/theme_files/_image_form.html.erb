<%= form_for @theme_file, url: create_image_admin_competition_theme_files_path(current_competition), html: { multipart: true } do |f| %>
  <%= render 'form_errors', object: @theme_file, label: 'theme file' %>

  <%= form_section 'Image file' do %>
    <%= f.file_field :image %>
  <% end %>

  <%= submit_button(f) %>
  <%= cancel_button(admin_competition_theme_files_path(current_competition)) %>
<% end %>
